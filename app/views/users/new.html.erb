<h1> Sign Up </h1>

<%= form_for @user do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :height %><br>
  <%= f.text_field :height %><br>
  <%= f.label :nausea %><br>
  <%= f.text_field :nausea %><br>
  <%= f.label :happiness %><br>
  <%= f.text_field :happiness %><br>
  <%= f.label :tickets %><br>
  <%= f.text_field :tickets %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <%= f.label :admin %><br>
  <%= f.check_box :admin %><br>
  <%= f.submit %>

<% end %>
